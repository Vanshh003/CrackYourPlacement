class Solution {
public:
    int removeDuplicates(vector<int> &nums) {
        if(nums.size() == 0) return 0;
        if(nums.size() == 1) return 1;
       
        int i = 0, j = 1, count = 1;
        while(j < nums.size()) {
            if(nums[j] == nums[i]) j++;
            else {
                i++;
                nums[i] = nums[j];
                
                count++;
            }
        }
        
        return count;
    }
};
