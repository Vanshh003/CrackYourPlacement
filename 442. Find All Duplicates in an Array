class Solution {
public:
    vector<int> findDuplicates(vector<int> &arr) {
        int n = arr.size();
        vector<int> ans;

        for(int i=0; i<n; i++) {
            if(arr[abs(arr[i]) - 1] < 0) ans.push_back(abs(arr[i]));
            else arr[abs(arr[i]) - 1] *= -1;
        }

        return ans;
    }
};
